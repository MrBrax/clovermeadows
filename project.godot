config_version=5

[application]

config/name="v-crossing"
run/main_scene="res://scenes/menu.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
boot_splash/image="res://ui/v.png"
boot_splash/use_filter=false
config/icon="res://icon.svg"

[autoload]

DebugDraw="*res://Code/DebugDraw.gd"
DependencyManager="*res://Code/Dependencies/DependencyManager.cs"
UiSounds="*res://Code/Ui/UiSounds.cs"
SettingsSaveData="*res://Code/Save/SettingsSaveData.cs"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[dialogue_manager]

general/uses_dotnet=true
general/balloon_path="res://dialogue/balloon.tscn"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
mouse_cursor/custom_image="res://icons/cursor/hand_point.png"
mouse_cursor/custom_image_hotspot=Vector2(10, 5)

[dotnet]

project/assembly_name="v-crossing"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/smoothing/plugin.cfg")

